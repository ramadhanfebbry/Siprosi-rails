<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <%= csrf_meta_tag %>
  <title>Siprosi</title>

  <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />


    <!-- Bootstrap core CSS     -->
        <link href="/assets/application.css" rel="stylesheet" />
    <link href="/assets/bootstrap.min.css" rel="stylesheet" />

    <!-- Animation library for notifications   -->
    <link href="/assets/animate.min.css" rel="stylesheet"/>

    <!--  Paper Dashboard core CSS    -->
    <link href="/assets/paper-dashboard.css" rel="stylesheet"/>


    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="/assets/demo.css" rel="stylesheet" />

    
    <!--  Fonts and icons     -->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Muli:400,300' rel='stylesheet' type='text/css'>
    <link href="/assets/themify-icons.css" rel="stylesheet">
    
        <!--   Core JS Files   -->
    <%= javascript_include_tag "application" %>
    

    <script src="/assets/bootstrap.min.js" type="text/javascript"></script>
   <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
 <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
 
    <!--  Checkbox, Radio & Switch Plugins -->
    <script src="/assets/bootstrap-checkbox-radio.js"></script>

    <!--  Charts Plugin -->

    <!--  Notifications Plugin    -->
    <script src="/assets/bootstrap-notify.js"></script>
    

    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.16.0/jquery.validate.min.js"></script>

</head>
<body>
    <%= render 'layouts/leftmenu' if user_signed_in?  %>
    <%= yield %>
<script type="text/javascript">
    $('.datepicker').datepicker();
    $(".stylish-input-group button").click(function(){
        var value=$('.stylish-input-group input').val();
        if(value.length > 0){
            document.location = '?q='+ $('.stylish-input-group input').val();      
        }else{
            document.location = '';
        }
      
    })


</script>
</body>
</html>
