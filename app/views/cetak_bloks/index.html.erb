<div class="card">
  <div class="header">
    <h4 class="title">Cetak Blok</h4>
    <p class="category">Here is a subtitle for this table</p>
  </div>
  <div class="content table-responsive table-full-width">
    <table class="table table-striped">
      <thead>
        <th>ID</th>
        <th>No. IP</th>        
        <th>Date</th>
        <th>Hasil</th>
        <th>Rusak</th>
        <th>Keterangan</th>        
        <th>Aksi</th>
      </thead>
      <tbody>
        <% @cetak_bloks.each do |cetak_blok| %>
        <tr>
          <td><%= cetak_blok.id %></td>
          <td><%= cetak_blok.ip_id %></td>
          <td><%= cetak_blok.date %></td>
          <td><%= cetak_blok.hasil %></td>
          <td><%= cetak_blok.rusak %></td>
          <td><%= cetak_blok.keterangan %></td>
          <td>
            <%= link_to 'Edit', edit_cetak_blok_path(cetak_blok), class: "btn btn-info btn-fill btn-sm" %>
            <%= link_to 'Destroy', cetak_blok, class: "btn btn-danger btn-fill btn-sm", data: {confirm: "Anda yakin akan menghapus data ini?"}, method: :delete %>
          </td>
        </tr>              
        <%end%>
      </tbody>
    </table>

  </div>
</div>
